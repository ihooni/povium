{
  "name": "povium",
  "version": "1.0.0",
  "description": "좋은 글 플랫폼",
  "main": "",
  "dependencies": {
    "autoprefixer": "^8.6.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.6.0",
    "nodemon": "^1.17.5",
    "postcss-cli": "^5.0.1"
  },
  "devDependencies": {
    "webpack": "^4.14.0",
    "webpack-command": "^0.4.1"
  },
  "scripts": {
    "webpack:build": "npx webpack --config webpack.config.js",
    "webpack:watch": "npx webpack --watch --config webpack.config.js",
    "autoprefix": "postcss public_html/src/build/css --use autoprefixer -d public_html/src/build/css",
    "sass:build": "concurrently \"node-sass -w public_html/src/scss/ -o public_html/src/build/css/\" \"nodemon -e scss -x npm run autoprefix\""
  },
  "browserslist": [
    "> 5%",
    "last 5 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DesignAndDevelop/Povium.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DesignAndDevelop/Povium/issues"
  },
  "homepage": "https://github.com/DesignAndDevelop/Povium#readme"
}
