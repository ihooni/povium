{
  "name": "povium",
  "version": "1.0.0",
  "description": "좋은 글 플랫폼",
  "main": "",
  "dependencies": {
    "autoprefixer": "^8.6.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.2",
    "postcss-cli": "^5.0.1"
  },
  "devDependencies": {
    "webpack": "^4.14.0",
    "webpack-command": "^0.4.1"
  },
  "scripts": {
    "webpack:build": "node_modules/.bin/webpack --config webpack.config.js",
    "webpack:watch": "node_modules/.bin/webpack --watch --config webpack.config.js",
    "autoprefix": "node_modules/.bin/postcss public_html/build/css --use autoprefixer -d public_html/build/css",
    "sass:build": "node_modules/.bin/node-sass resources/assets/scss/ -o public_html/build/css/",
    "sass:build:autoprefix": "npm run sass:build && npm run autoprefix",
    "sass:build:autoprefix:watch": "node_modules/.bin/nodemon -e scss -x npm run sass:build:autoprefix"
  },
  "browserslist": [
    "> 5%",
    "last 5 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DesignAndDevelop/Povium.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DesignAndDevelop/Povium/issues"
  },
  "homepage": "https://github.com/DesignAndDevelop/Povium#readme"
}
