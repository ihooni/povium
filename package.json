{
  "name": "povium",
  "version": "1.0.0",
  "description": "좋은 글 플랫폼",
  "main": "",
  "dependencies": {
    "autoprefixer": "^8.6.3",
    "concurrently": "^3.6.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "postcss-cli": "^5.0.1"
  },
  "devDependencies": {
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "scripts": {
	"webpack:build": "webpack --config webpack.config.js",
	"autoprefix": "postcss public_html/src/build/css --use autoprefixer -d public_html/src/build/css",
	"sass:build": "concurrently \"node-sass -w public_html/src/scss/ -o public_html/src/build/css/\" \"nodemon -e scss -x npm run autoprefix\""
  },
  "browserslist": [
    "> 5%",
    "last 5 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DesignAndDevelop/Povium.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DesignAndDevelop/Povium/issues"
  },
  "homepage": "https://github.com/DesignAndDevelop/Povium#readme"
}
