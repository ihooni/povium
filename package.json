{
  "name": "povium",
  "version": "1.0.0",
  "description": "좋은 글 플랫폼",
  "main": "",
  "dependencies": {
    "axios": "^0.18.0",
    "detect-browser": "^4.0.3",
    "gsap": "^2.0.2",
    "node-notifier": "^5.2.1",
    "pjax": "^0.2.7",
    "vue": "^2.5.18"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@types/gsap": "^1.20.0",
    "@types/node": "^10.12.7",
    "autoprefixer": "^9.4.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "nodemon": "^1.18.8",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "webpack:build": "node_modules/.bin/webpack --config webpack.config.js",
    "webpack:watch": "node_modules/.bin/webpack --watch --config webpack.config.js",
    "autoprefix": "node_modules/.bin/postcss public_html/build/css/*.css --use autoprefixer -d public_html/build/css/",
    "sass:build": "node_modules/.bin/node-sass resources/assets/scss/ -o public_html/build/css/ --source-map true",
    "sass:watch": "node_modules/.bin/node-sass resources/assets/scss/ -o public_html/build/css/ -w --source-map true",
    "less:build": "node_modules/.bin/lessc resources/assets/less/povium.style.less public_html/build/css/povium.style.css --source-map",
    "init": "npm run webpack:build && npm run less:build && npm run autoprefix",
    "frontend:watch": "node_modules/.bin/npm-run-all --parallel webpack:watch css:watch",
    "frontend:build": "node_modules/.bin/npm-run-all --parallel webpack:build css:build",
    "notify": "node notification.js hello world",
    "css:watch": "node_modules/.bin/nodemon -e less -x \"npm run less:build && npm run autoprefix\"",
    "css:build": "npm run less:build && npm run autoprefix"
  },
  "browserslist": [
    "> 5%",
    "last 5 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Povium/Readigm/Readigm-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/Povium/Readigm/Readigm-web/issues"
  }
}
